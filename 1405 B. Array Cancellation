#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long t,n,i,s,y=0;
    cin>>t;
    while(t--)
    {
        cin>>n;
        s=0;
        while(n--)
        {
            cin>>i;
            if(i>0)
            {
                s=s+i;
            }
            else
            {
                s=max(y,s+i);
            }
        }
        cout<<s<<"\n";
    }
}
